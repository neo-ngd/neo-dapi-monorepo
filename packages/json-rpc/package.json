{
  "name": "@neongd/json-rpc",
  "version": "0.0.1",
  "description": "Json Rpc types and toolings",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:types build:code",
    "build:code": "rollup -c",
    "build:types": "tsc -p tsconfig.build.json",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --ext ts,js",
    "lint:fix": "eslint . --ext ts,js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm-run-all --parallel 'build:types -- -w' 'build:code -- -w'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-ngd/neo-dapi.git"
  },
  "keywords": [
    "jsonrpc"
  ],
  "author": "neo-ngd <github.com/neo-ngd>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neo-ngd/neo-dapi/issues"
  },
  "homepage": "https://github.com/neo-ngd/neo-dapi#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "safe-json-utils": "^1.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
