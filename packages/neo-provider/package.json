{
  "name": "@neongd/neo-provider",
  "version": "0.0.12-alpha.0",
  "description": "Neo Provider types and toolings",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-s build:cjs build:esm build:umd",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build:umd": "webpack",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests",
    "watch": "tsc -p tsconfig.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-ngd/neo-dapi-monorepo.git"
  },
  "keywords": [
    "neo-provider"
  ],
  "author": "neo-ngd <github.com/neo-ngd>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neo-ngd/neo-dapi-monorepo/issues"
  },
  "homepage": "https://github.com/neo-ngd/neo-dapi-monorepo",
  "dependencies": {
    "@neongd/json-rpc": "^0.0.11",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
